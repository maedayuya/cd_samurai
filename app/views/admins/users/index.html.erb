<h1> ユーザ一覧</h1>

<table class="table table-hover table-inverse">
　<thead>
	<tr>
  	  <th>ユーザID</th>
  	  <th>ユーザ名</th>
  	  <th>メールアドレス</th>
  	  <th colspan="3"></th>
  	</tr>
  </thead>

  <tbody>
	<% @users.each do |user| %>
  	<tr>
  	  <td><%= user.id %></td>
	  <td><%= user.last_name + user.first_name %></td>
	  <td><%= user.email %></td>

	  <td><%= link_to "詳細" , admins_user_path(user) %> ｜
		  <%= link_to "編集" , edit_admins_user_path(user) %> ｜
		  <%= link_to "削除" , admins_user_path(user), method: :delete %><br>
	  </td>
	<% end %>
    </tr>
  </tbody>
</table>